{
  "name": "egg-ssr",
  "version": "1.0.0",
  "dependencies": {
    "egg": "^2.21.0",
    "egg-bin": "^4.12.3",
    "egg-proxy": "^1.1.0",
    "egg-scripts": "^2.11.0",
    "egg-static": "^2.2.0",
    "koa-router": "^7.4.0",
    "react": "^16.8.3",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.8.3",
    "react-router-dom": "^5.0.0",
    "ykfe-utils": "^1.1.0"
  },
  "scripts": {
    "debug": "egg-bin debug",
    "standard": "standard",
    "start": "concurrently \"npm run ssr\" \" npm run csr \"",
    "ssr": "rm -rf dist && concurrently \"egg-bin dev\" \"NODE_ENV=development webpack --watch --config ./build/webpack.config.server.js\"",
    "csr": "NODE_ENV=development webpack-dev-server --port 8000 --hot --client-log-level error --config ./build/webpack.config.client.js",
    "prod": "npm run build && eggctl start",
    "build:server": "NODE_ENV=production webpack --config build/webpack.config.server.js",
    "build": "rm -rf dist && NODE_ENV=production webpack --config build/webpack.config.client.js && npm run build:server",
    "analyze": "NODE_ENV=production npm_config_report=true npm run build"
  },
  "pre-commit": [
    "standard"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "node_modules",
      "app/public",
      "dist"
    ],
    "global": [
      "__isBrowser__",
      "location",
      "window"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": []
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@svgr/webpack": "2.4.1",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.4",
    "concurrently": "^4.1.0",
    "css-hot-loader": "^1.4.3",
    "css-loader": "1.0.0",
    "css-modules-require-hook": "^4.2.3",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.0.5",
    "postcss-safe-parser": "4.0.1",
    "pre-commit": "^1.2.2",
    "standard": "^12.0.1",
    "terser-webpack-plugin": "^1.2.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "3.3.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}